{
  "name": "react-hooks-fetch",
  "description": "A React custom hook for data fetching with Suspense",
  "version": "0.11.0-experimental.1",
  "author": "Daishi Kato",
  "repository": {
    "type": "git",
    "url": "https://github.com/dai-shi/react-hooks-fetch.git"
  },
  "main": "./dist/index.js",
  "module": "./src/index.js",
  "types": "./src/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "compile": "babel src -d dist",
    "test": "npm run eslint && npm run tsc-test && npm run jest",
    "eslint": "eslint --ext .js,.ts,.tsx --ignore-pattern dist --ignore-pattern mdx-deck .",
    "jest": "jest",
    "tsc-test": "tsc --project . --noEmit",
    "examples:minimal": "DIR=01_minimal EXT=js webpack-dev-server",
    "examples:typescript": "DIR=02_typescript webpack-dev-server"
  },
  "keywords": [
    "react",
    "hooks",
    "fetch",
    "suspense"
  ],
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/react": "^9.1.3",
    "@types/node-fetch": "^2.5.0",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "core-js": "^3.2.1",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^2.1.2",
    "react": "experimental",
    "react-dom": "experimental",
    "ts-loader": "^6.1.0",
    "typescript": "^3.6.3",
    "use-memo-one": "^1.1.1",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": 3
        }
      ],
      "@babel/preset-react"
    ]
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ]
  }
}
